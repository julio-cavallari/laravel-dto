{
  "name": "julio-cavallari/laravel-dto",
  "description": "Laravel package to automatically generate DTOs from Form Request classes",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "dto",
    "data-transfer-object",
    "form-request",
    "code-generation"
  ],
  "authors": [
    {
      "name": "Julio Cavallari",
      "email": "julio@example.com"
    }
  ],
  "require": {
    "php": "^8.2|^8.3|^8.4",
    "laravel/framework": "^11.0|^12.0",
    "illuminate/console": "^11.0|^12.0",
    "illuminate/support": "^11.0|^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "pestphp/pest": "^2.0",
    "pestphp/pest-plugin-laravel": "^2.0",
    "orchestra/testbench": "^9.0",
    "phpstan/phpstan": "^1.0",
    "laravel/pint": "^1.0",
    "rector/rector": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "JulioCavallari\\LaravelDto\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "JulioCavallari\\LaravelDto\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "JulioCavallari\\LaravelDto\\LaravelDtoServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "pest",
    "test:unit": "pest --testsuite=Unit",
    "test:coverage": "pest --coverage",
    "analyse": "phpstan analyse",
    "format": "pint",
    "format:test": "pint --test",
    "refactor": "rector",
    "refactor:dry": "rector --dry-run",
    "quality": [
      "@format",
      "@analyse",
      "@test"
    ]
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}